{
  "name": "editor",
  "author": "narutopig",
  "version": "1.0.0",
  "private": true,
  "homepage": "./",
  "main": "build/electron.js",
  "dependencies": {
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.3.0",
    "@material-ui/core": "^5.0.0-beta.5",
    "@monaco-editor/react": "^4.3.1",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-markdown": "^7.1.0",
    "react-scripts": "^3.4.4",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "estart": "electron build/electron.js",
    "package-osx": "electron-packager . \"Markdown Editor\" --overwrite --platform=darwin --arch=x64 --prune=true --out=dist",
    "package-win": "electron-packager . \"Markdown Editor\" --overwrite --platform=win32 --arch=x64 --prune=true --out=dist",
    "package-linux": "electron-packager . \"Markdown Editor\" --overwrite --platform=linux --arch=x64 --prune=true --out=dist"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "^15.3.0",
    "electron-packager": "^15.4.0"
  }
}
